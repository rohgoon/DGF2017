<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="biz.model.BmFesInfoViewDao">
	<select id="selectListAll" resultType="BmFesInfoView">
		select * from bmFesInfoView
