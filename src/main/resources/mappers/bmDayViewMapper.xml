<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="biz.model.BmDayViewDao">
	<select id="selectListAll" resultType="BmDayView">
		select * from bmDayView
	</select>
	<select id="selectListByYear" parameterType="string" resultType="BmDayView">
		select * from bmDayView where ymdDate &gt;= #{param1} and ymdDate &lt;=#{param2}		
	</select>
</mapper>