<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="biz.model.BmMonthViewDao">
	<select id="selectListAll" resultType="bmMonthView">
		select * from bmMonthView
	</select>
	<select id="selectListByYear" parameterType="int" resultType="bmMonthView">
		select * from bmMonthView where year &gt;= #{param1} and month &gt;=#{param2} and year &lt;= #{param3} and month &gt;=#{param4} 
	</select>
</mapper>