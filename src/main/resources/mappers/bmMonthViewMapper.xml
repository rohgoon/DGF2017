<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="biz.model.BmMonthViewDao">
	<select id="selectListAll" resultType="bmMonthView">
		select * from bmMonthView
	</select>
	<select id="selectListByYmDate" parameterType="string" resultType="bmMonthView">
		select * from bmMonthView where ymDate &gt;= #{param1} and ymDate &lt;=#{param2} 
	</select>
</mapper>