<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="reservation.model.RCountViewDao">
    <select id="selectListByUno" parameterType="int" resultType="RCountView">
        select * from rCountView where uno =#{uno}
    </select>
    <select id="selectListByRno" parameterType="int" resultType="RCountView">
        select * from rCountView where rno = #{rno}
    </select>
</mapper>