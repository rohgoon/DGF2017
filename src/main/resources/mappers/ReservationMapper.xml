<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="festival.model.ReservationDao"> 
  <insert id="insert" parameterType="reservation">
      insert into reservation(id,uname,upassword,email,phone, regdate) values(#{id},#{uname},#{upassword},#{email},#{phone},#{regDate})
  </insert>
</mapper>